{
    "name": "bfuzze/laravel-test-factory-helper",
    "description": "Generate Laravel test factories from your existing models",
    "keywords": ["Laravel", "Tests", "Factory"],
    "license": "MIT",
    "require": {
        "php": "^8",
        "illuminate/support": "^8.0|^9.0|^10.0",
        "illuminate/console": "^8.0|^9.0|^10.0",
        "illuminate/filesystem": "^8.0|^9.0|^10.0",
        "doctrine/dbal": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "bfuzze\\LaravelTestFactoryHelper\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "bfuzze\\LaravelTestFactoryHelper\\TestFactoryHelperServiceProvider"
            ]
        }
    }
}
